{
  "name": "electron-boilerplate",
  "productName": "Electron Boilerplate",
  "description": "Starter for your Electron application",
  "version": "0.0.0",
  "author": "Yu Narukaze",
  "copyright": "Â© 2017, Yu Narukaze",
  "license": "MIT",
  "main": "app/background.js",
  "build": {
    "appId": "com.example.electron-boilerplate",
    "files": [
      "app/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "publish": null
  },
  "scripts": {
    "postinstall": "install-app-deps",
    "build": "gulp build",
    "prerelease": "gulp build --env=production",
    "release": "build",
    "start": "gulp start",
    "pretest": "gulp build-unit",
    "test": "karma start .karma-config/karma.conf.js",
    "coverage": "karma start .karma-config/karma.coverage.conf.js",
    "pree2e": "gulp build-e2e --env=test",
    "e2e": "gulp run-e2e"
  },
  "dependencies": {
    "angular": "1.5.1",
    "angular-ui-router": "^0.4.2",
    "fs-jetpack": "^1.0.0",
    "font-awesome": "^4.7.0",
    "ui-router-extras": "^0.1.3"
  },
  "devDependencies": {
    "angular-mocks": "^1.5.1",
    "chai": "^4.1.0",
    "electron": "^1.6.11",
    "electron-builder": "^19.16.0",
    "gulp": "^3.9.1",
    "gulp-batch": "^1.0.5",
    "gulp-plumber": "^1.1.0",
    "gulp-sass": "^3.1.0",
    "gulp-util": "^3.0.8",
    "gulp-wait": "0.0.2",
    "gulp-watch": "^4.3.11",
    "istanbul": "^0.4.5",
    "jasmine": "^2.7.0",
    "jasmine-core": "^2.7.0",
    "karma": "^1.7.0",
    "karma-coverage": "^1.1.1",
    "karma-electron": "^5.2.1",
    "karma-jasmine": "^1.1.0",
    "karma-rollup-preprocessor": "^4.0.2",
    "minimist": "^1.2.0",
    "mocha": "^3.4.2",
    "rollup": "^0.45.2",
    "rollup-plugin-istanbul": "^1.1.0",
    "source-map-support": "^0.4.15",
    "spectron": "^3.7.2"
  }
}
